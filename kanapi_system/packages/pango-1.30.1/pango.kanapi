PACKAGE_NAME=pango
VER=1.30.1
URL="http://ftp.gnome.org/pub/gnome/sources/pango/1.30"
EXT=tar.xz
#build depends on

LIB_DEPENDS="x11 xcb xau ice"

. ../settings

export CPP="gcc -E ${CFLAGS}"

pkg_prepare() {
